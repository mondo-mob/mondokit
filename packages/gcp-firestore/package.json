    "@google-cloud/firestore": ">= 6.5.0",
    "@mondokit/gcp-core": "1.0.1"
  },
  "devDependencies": {
    "@google-cloud/firestore": "^7.11.6",
    "@mondokit/gcp-core": "1.0.1",
    "@types/lodash-es": "^4.17.8"
  },
  "keywords": [
    "firestore",
